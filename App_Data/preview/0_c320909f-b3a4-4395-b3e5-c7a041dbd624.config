            <node id="2036" parentID="2037" level="6" writerID="0" creatorID="1" nodeType="1284" template="1277" sortOrder="1" createDate="2013-06-18T14:47:52" updateDate="2013-10-07T14:19:43" nodeName="Tree and human health may be linked" urlName="tree-and-human-health-may-be-linked" writerName="Administrator" creatorName="content" nodeTypeAlias="BlogPost" path="-1,1244,1289,1901,2023,2037,2036">
              <data alias="SEOTitle" />
              <data alias="metaKeywords" />
