            <node id="1938" parentID="1939" level="6" writerID="0" creatorID="1" nodeType="1284" template="1277" sortOrder="1" createDate="2013-02-28T10:31:59" updateDate="2013-10-07T14:19:27" nodeName="Simcro supporting our work" urlName="simcro-supporting-our-work" writerName="Administrator" creatorName="content" nodeTypeAlias="BlogPost" path="-1,1244,1289,1901,1921,1939,1938">
