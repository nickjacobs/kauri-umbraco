          <node id="1937" parentID="1921" level="5" writerID="0" creatorID="1" nodeType="1288" template="0" sortOrder="23" createDate="2013-02-28T10:30:46" updateDate="2013-10-07T14:19:26" nodeName="23" urlName="23" writerName="Administrator" creatorName="content" nodeTypeAlias="DateFolder" path="-1,1244,1289,1901,1921,1937">
            <node id="1935" parentID="1937" level="6" writerID="0" creatorID="1" nodeType="1284" template="1277" sortOrder="1" createDate="2013-02-28T10:26:47" updateDate="2013-10-07T14:19:26" nodeName="TV3 news focus on future funding" urlName="tv3-news-focus-on-future-funding" writerName="Administrator" creatorName="content" nodeTypeAlias="BlogPost" path="-1,1244,1289,1901,1921,1937,1935">
