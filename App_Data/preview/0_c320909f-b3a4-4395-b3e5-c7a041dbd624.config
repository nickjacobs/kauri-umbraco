            <node id="1989" parentID="1985" level="6" writerID="0" creatorID="1" nodeType="1284" template="1277" sortOrder="3" createDate="2013-04-17T22:32:27" updateDate="2013-10-07T14:19:33" nodeName="&quot;Kauri icons threatened by inaction&quot;" urlName="kauri-icons-threatened-by-inaction" writerName="Administrator" creatorName="content" nodeTypeAlias="BlogPost" path="-1,1244,1289,1901,1978,1985,1989">
