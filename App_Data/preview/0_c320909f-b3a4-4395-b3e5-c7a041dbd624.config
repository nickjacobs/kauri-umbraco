            <node id="1635" parentID="1637" level="6" writerID="0" creatorID="1" nodeType="1284" template="1277" sortOrder="1" createDate="2012-02-22T14:50:38" updateDate="2013-10-07T14:18:54" nodeName="&quot;Nature's best&quot; at Waitangi celebration" urlName="nature's-best-at-waitangi-celebration" writerName="Administrator" creatorName="content" nodeTypeAlias="BlogPost" path="-1,1244,1289,1622,1629,1637,1635">
              <data alias="SEOTitle" />
              <data alias="metaKeywords" />
