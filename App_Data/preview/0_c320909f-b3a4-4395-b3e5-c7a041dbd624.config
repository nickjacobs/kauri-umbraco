            <node id="2468" parentID="2470" level="6" writerID="1" creatorID="1" nodeType="1284" template="1277" sortOrder="1" createDate="2015-07-09T08:40:49" updateDate="2015-07-08T20:56:16" nodeName="Wellsford School explore kauri" urlName="wellsford-school-explore-kauri" writerName="content" creatorName="content" nodeTypeAlias="BlogPost" path="-1,1244,1289,2429,2469,2470,2468">
              <data alias="SEOTitle" />
