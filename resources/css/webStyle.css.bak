/*~~~~~~~~~~~~~~~~~global styles start~~~~~~~~~~~~~~~~~*/
html {
	width: 100%;
	height: 100%;
	_border: none;
}
body, form {
	margin: 0px;
	width: 100%;
	height: 100%;
}
body {
	background: url(../images/ui/bgBody.jpg) top center repeat #9EB4AF;
}
img, object, embed {
	display: block;
	outline: none;
}
a img, img a, img {
	border: none;
}
a {
	outline: none;
	text-decoration: none;
}
h1, h2, h3, h4, h5, h6, p, ul, ol, li {
	padding: 0;
	margin: 0;
}

h1 {
	font-family: 'dinprolight', Arial, Helvetica, sans-serif;
	font-size: 37px;
	line-height: 44px;
	color: #3F433E;
	font-weight: normal;
	margin-bottom: 15px;
}
h2 {
	font-family: 'dinprolight', Arial, Helvetica, sans-serif;
	font-size: 23px;
	line-height: 26px;
	color: #3F433E;
	font-weight: normal;
	margin-bottom: 23px;
}
h3 {
	font-family: 'dinprobold', Arial, Helvetica, sans-serif;
	font-size: 18px;
	line-height: 23px;
	color: #3F433E;
	font-weight: normal;
	margin-bottom: 12px;
}
h4 {
	font-family: 'dinprobold', Arial, Helvetica, sans-serif;
	font-size: 14px;
	line-height: 14px;
	font-weight: normal;
	color: #3F433E;
}
h5 {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 14px;
	line-height: 14px;
	font-weight: normal;
	color: #999999;
}

p {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 14px;
	line-height: 23px;
	color: #3F433E;
	margin-bottom: 20px;
}

p a, li a {
	text-decoration: underline;
	color: #3F433E;
}
p a:hover, li a:hover {
	color: #FDB913;
}

p.error, p.success {
	display: none;
	clear: both;
	margin-bottom: 5px;
	text-align: left;
	color: red;
	background: #FAD4D9;
	padding: 2px 4px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;
	border-radius: 6px;
	behavior: url(PIE.htc);
	position: relative;
	font-family:Arial,Helvetica,sans-serif;
	font-size:12px;
}

p.error {
	color: red !important;
	background: #FAD4D9;
}

p.success {
	color: green !important;
	background: #DFFFD7;
}



p.dl_error, p.dl_success {
	display: none;
	clear: both;
	margin-bottom: 5px;
	text-align: left;
	color: red;
	background: #FAD4D9;
	padding: 2px 4px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;
	border-radius: 6px;
	behavior: url(PIE.htc);
	position: relative;
	font-family:Arial,Helvetica,sans-serif;
	font-size:12px;
}

p.dl_error {
	color: red !important;
	background: #FAD4D9;
}

p.dl_success {
	color: green !important;
	background: #DFFFD7;
}


fieldset {
	border: none;
	padding: 0;
	margin: 0;
	outline: 0;
}

.clearer{
	clear: both;
	line-height: 0px;
}
.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}
p.noJS {
    font-family: Trebuchet MS, Verdana, Arial, Helvetica, sans-serif;
    font-size: 10px;
	color: #FFF;
    display: block;
    width: 100%;
    padding: 4px 0 3px 0;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 100;
    background-color: #F24E31;
}

/*~~~~~~~~~~~~~~~~~ FONTS ~~~~~~~~~~~~~~~~~*/
@font-face {
	font-family: 'dinpromed';
	src: url('../fonts/DINPro-Medium.eot');
	src: local('?'), 
	url('../fonts/DINPro-Medium.woff') format('woff'),
	url('../fonts/DINPro-Medium.ttf') format('truetype'),
	url('../fonts/DINPro-Medium.svg') format('svg');
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'dinprobold';
	src: url('../fonts/DINPro-Bold.eot');
	src: local('?'), 
	url('../fonts/DINPro-Bold.woff') format('woff'), 
	url('../fonts/DINPro-Bold.ttf') format('truetype'), 
	url('../fonts/DINPro-Bold.svg') format('svg');
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'dinprolight';
	src: url('../fonts/DINPro-Light.eot');
	src: local('?'), 
	url('../fonts/DINPro-Light.woff') format('woff'), 
	url('../fonts/DINPro-Light.ttf') format('truetype'), 
	url('../fonts/DINPro-Light.svg') format('svg');
	font-weight: normal;
	font-style: normal;
}

/***** Lists *****/
ul {
	margin: -10px 0 10px 20px;
	list-style:none;
}
ul li{
	margin: 0 0 8px 0;
	padding:  0 0 0 12px;
	background:url(../Images/UI/bullet.gif) no-repeat 0 6px;	
	font-family: Arial, Helvetica, sans-serif;
	font-size: 14px;
	line-height: 18px;
	color: #3F433E;
}

ol {
	margin: 10px 0 30px 30px;
}
ol li{
	margin: 0 0 5px 0;
	padding:  0 ;
	font-size:13px;
	color:#333;
	line-height:160%;
}

/***** Tables *****/
table{
	margin:0 0 20px 0;
	color:#666;
	font-size:13px;
	background:#EEE;
}
th{
	padding:3px;	
	background:#FFF;
}
td{
	padding:3px;	
	background:#FFF;
}
/***** Global Classes *****/
.clear         { clear:both; }
.float-left    { float:left; }
.float-right   { float:right; }
 
.text-left     { text-align:left; }
.text-right    { text-align:right; }
.text-center   { text-align:center; }
.text-justify  { text-align:justify; }
 
.bold          { font-weight:bold; }
.italic        { font-style:italic; }
.underline     { border-bottom:1px solid; }
.highlight     { background:#ffc; }
 
.wrap          { width:960px;margin:0 auto; }
 
.img-left      { float:left;margin:4px 10px 4px 0; }
.img-right     { float:right;margin:4px 0 4px 10px; }
 
.nopadding     { padding:0; }
.noindent      { margin-left:0;padding-left:0; }
.nobullet      { list-style:none;list-style-image:none; }

.noLink
{
     cursor:default;    
    }

/*~~~~~~~~~~~~~~~~~global styles end~~~~~~~~~~~~~~~~~*/

